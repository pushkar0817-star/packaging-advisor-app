  "packaging_levels": {
    "Primary": {
      "description": "Packaging that directly encloses the product to protect and contain it. It is in direct contact with the product.",
      "examples": [
        "Plastic bottle for soft drink",
        "Blister pack for tablets",
        "Tetra Pak for milk",
        "Pouch for chips"
      ]
    },
    "Secondary": {
      "description": "Packaging that groups primary packages together for handling, branding, and display. Provides additional protection.",
      "examples": [
        "Paperboard carton holding toothpaste tubes",
        "Shrink wrap around water bottles",
        "Box of medicine blister packs",
        "Carton for juice tetra packs"
      ]
    },
    "Tertiary": {
      "description": "Packaging used for bulk handling, storage, and transportation. Ensures safe delivery of secondary packages.",
      "examples": [
        "Corrugated fiberboard box for shipping",
        "Wooden pallet with stretch wrap",
        "Large corrugated cartons containing smaller cartons",
        "Crates used in exports"
      ]
    }
  }
}
